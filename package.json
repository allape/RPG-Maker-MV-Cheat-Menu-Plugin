{
  "name": "rpg-maker-mv-cheater",
  "version": "0.5.2",
  "description": "Refined cheat plugin for RPG Maker MV/MZ",
  "private": true,
  "scripts": {
    "prepare:clean": "npx rimraf build/ dist/",
    "build:ts": "npx tsc",
    "build:webpack": "npm run build:ts && npx webpack",
    "build:webpack:legacy": "npm run build:ts && npx cross-env BUILD_LEGACY=true npx webpack",
    "build:plugin": "npm run prepare:clean && npm run build:webpack && npm run postbuild",
    "build:plugin:legacy": "npm run prepare:clean && npm run build:webpack:legacy && npm run postbuild",
    "build:go": "cd patcher && ./build.sh",
    "build": "npm run prepare:clean && npm run build:webpack && npm run build:webpack:legacy && npm run build:go",
    "postbuild": "node postbuild.js"
  },
  "keywords": [
    "translate",
    "speed hack",
    "cheat",
    "RPG-Maker-MV",
    "RPG-Maker-MZ"
  ],
  "author": "allape",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.7.0",
    "rimraf": "^3.0.2",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "typescript": "^4.4.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "core-js": "^3.21.0",
    "element-polyfill": "^1.1.0",
    "regenerator-runtime": "^0.13.9",
    "whatwg-fetch": "^3.6.2"
  }
}
